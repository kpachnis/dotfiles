! xterm
.xterm*borderColor           :#000000
.xterm*internalBorder        :4

.xterm*Scrollbar.foreground  :gray50
.xterm*Scrollbar.background  :#000000
.xterm*Scrollbar.minimumThumb:50

.xterm*faceName              :Fantasque Sans Mono
.xterm*faceSize              :13

.xterm*saveLines             :10000
.xterm*scrollBar             :true
.xterm*scrollbar*width       :10

.xterm*selectToClipboard     :true
.xterm*utmpInhibit           :true
.xterm*pointerMode           :2

.xterm*title                 :xterm
.xterm*termName              :xterm-256color

.xterm*visualBell            :false

.xterm*Translations          :#override \
        Ctrl Shift <Key>V    :insert-selection(CLIPBOARD) \n\
        Ctrl Shift <Key>C    :copy-selection(CLIPBOARD)


!IR_Black colorscheme
.xterm*foreground            :#f1f1f1
.xterm*background            :#000000
.xterm*cursorColor           :#808080

!Black
.xterm*color0                :#4f4f4f
.xterm*color8                :#7b7b7b

!Red
.xterm*color1                :#fa6c60
.xterm*color9                :#fcb6b0

!Green
.xterm*color2                :#a8ff60
.xterm*color10               :#cfffab

!Yellow
.xterm*color3                :#fffeb7
.xterm*color11               :#ffffcc

!Blue
.xterm*color4                :#96cafe
.xterm*color12               :#b5dcff

!Magenta
.xterm*color5                :#fa73fd
.xterm*color13               :#fb9cfe

!Cyan
.xterm*color6                :#c6c5fe
.xterm*color14               :#e0e0fe

!White
.xterm*color7                :#efedef
.xterm*color15               :#ffffff

!Bold, Italic, Underline
.xterm*colorBD               :#ffffff

! XScreenSaver
xscreensaver.splash          :false
